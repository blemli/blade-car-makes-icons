{
    "name": "johan-boshoff/blade-car-makes-icons",
    "description": "A package to easily make use of car-makes-icons in your Laravel Blade views.",
    "keywords": ["Blade", "Icons", "Laravel", "Car", "Vehicle", "Makes", "Manufacturers", "Logos", "Laravel"],
    "homepage": "https://github.com/johan-boshoff/blade-car-makes-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Boshoff"
        }
    ],
    "require": {
        "php": "^8.3",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "codeat3/blade-icon-generation-helpers": "^0.10.0",
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "JohanBoshoff\\CarMakesIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JohanBoshoff\\CarMakesIcons\\BladeCarMakesIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
